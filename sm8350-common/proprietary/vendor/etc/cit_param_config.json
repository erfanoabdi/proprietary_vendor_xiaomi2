            "config":{
                "first_sar_chanel_num": 6,
                "second_sar_chanel_num": 6,
                "c0_threshold_far_first_sar":[0,200],
                "c0_threshold_near_first_sar":[2000,30000],
                "c0_threshold_far_second_sar":[0,200],
                "c0_threshold_near_second_sar":[2000,30000]
            }

        },
        {
            "name":"home_fp_sensor_check_test",
            "caption":"指纹传感器测试"
        },
        {
            "name":"home_fp_fod_check_test",
            "caption":"屏下指纹录入测试",
            "config":{
                "finger_print_tips_view_color_generation":3,
                "finger_print_tips_view_shape":"circle",
                "fp_fod_test_hbm_switch_on_cmd":"echo 5  1024 >  /sys/class/mi_display/disp-DSI-0/disp_param",
                "fp_fod_test_hbm_switch_off_cmd":"echo 5  -1 >  /sys/class/mi_display/disp-DSI-0/disp_param"
            }
        }

